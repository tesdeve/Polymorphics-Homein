<p id="notice"><%= notice %></p>

<h1>Conjuntos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conjuntos.each do |conjunto| %>
      <tr>
        <td><%= conjunto.nombre %></td>
        <td><%= link_to 'Show', conjunto %></td>
        <td><%= link_to 'Edit', edit_conjunto_path(conjunto) %></td>
        <td><%= link_to 'Destroy', conjunto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Conjunto', new_conjunto_path %>
